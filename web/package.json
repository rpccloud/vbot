{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.9",
    "antd": "^4.16.13",
    "check-password-strength": "^2.0.3",
    "mobx": "^6.3.3",
    "mobx-react": "^7.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "rpccloud-client-js": "^0.0.13",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "node-sass": "^6.0.1",
    "sass-loader": "^12.1.0"
  },
  "scripts": {
    "antd": "cd ./node_modules/antd/dist/ && cp antd.css antd.scss && cp antd.dark.css antd.dark.scss",
    "sass:release": "node-sass --output-style compressed src/theme/export -o public",
    "sass:watch": "node-sass src/theme/export -wo public",
    "prestart": "yarn antd",
    "start": "concurrently --kill-others \"react-scripts start\" \"yarn sass:watch\"",
    "prebuild": "yarn antd && yarn sass:release",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
